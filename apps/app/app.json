{
  "expo": {
    "name": "Whisper",
    "slug": "whisper",
    "scheme": "whisper",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "backgroundColor": "#6750A4"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.baqazi.whisper",
      "googleServicesFile": "./config/firebase/GoogleService-Info.plist"
    },
    "android": {
      "package": "com.baqazi.whisper",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "googleServicesFile": "./config/firebase/google-services.json",
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "onesignal-expo-plugin",
        {
          "mode": "development"
        }
      ],
      "expo-apple-authentication"
    ],
    "extra": {
      "supportsRTL": true,
      "oneSignalAppId": "65eb2508-a403-4157-b52f-528a39caf8e4",
      "eas": {
        "projectId": "f7e30c6c-f300-4899-9f2b-49eb84bf0831"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/f7e30c6c-f300-4899-9f2b-49eb84bf0831"
    }
  }
}
