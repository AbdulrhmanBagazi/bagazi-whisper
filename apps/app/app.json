{
  "expo": {
    "name": "Whisper",
    "slug": "whisper",
    "scheme": "whisper",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#6750A4"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.baqazi.whisper",
      "googleServicesFile": "./config/firebase/GoogleService-Info.plist"
    },
    "android": {
      "package": "com.baqazi.whisper",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "googleServicesFile": "./config/firebase/google-services.json"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "onesignal-expo-plugin",
        {
          "mode": "development"
        }
      ],
      "expo-apple-authentication"
    ],
    "extra": {
      "supportsRTL": true,
      "oneSignalAppId": "7bcd247a-1208-4225-93c4-5716ee213644",
      "eas": {
        "build": {
          "experimental": {
            "ios": {
              "appExtensions": [
                {
                  "targetName": "OneSignalNotificationServiceExtension",
                  "bundleIdentifier": "com.baqazi.whisper.OneSignalNotificationServiceExtension",
                  "entitlements": {
                    "com.apple.security.application-groups": [
                      "group.com.baqazi.whisper.onesignal"
                    ]
                  }
                }
              ]
            }
          }
        },
        "projectId": "f7e30c6c-f300-4899-9f2b-49eb84bf0831"
      }
    },
    "build": {
      "preview": {
        "distribution": "internal",
        "android": {
          "buildType": "apk"
        },
        "ios": {
          "simulator": true
        }
      }
    }
  }
}
